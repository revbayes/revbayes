seed(1)

t ~ dnNormal(0,10)
t.setValue(1)
        
a ~ dnNormal(-2,2)
a.setValue(0)

fossil ~ dnPseudo(t, pdAbove(a))
fossil.clamp( pseudoObservation() )

fossil2 ~ dnPseudo(t, pdLogNormal(0,1))
fossil2.clamp( pseudoObservation() )

mymodel = model(t,a)
mymodel.graph("output/logNormal.dot")

mymcmc = mcmc(mymodel,moves=[mvSlice(t),mvSlice(a)],monitors=[mnModel(filename="output/logNormal.log")])
mymcmc.run(1000)
q()
